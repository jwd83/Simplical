<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_onboard"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="me.jwd.simplical.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="me.jwd.simplical.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_UpdateGoalWeight"
            app:destination="@id/UpdateGoalWeight" />
    </fragment>

    <fragment
        android:id="@+id/UpdateGoalWeight"
        android:name="me.jwd.simplical.GoalWeightFragment"
        android:label="Onboard Goal Weight"
        tools:layout="@layout/fragment_goal_weight"
        >

    </fragment>

